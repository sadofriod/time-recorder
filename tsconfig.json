{
  "compilerOptions": {
    "target": "es5",                  
    "module": "commonjs",                                       
    "sourceMap": true,                  
    "outDir": "./bundle",                 
    "strict": true,                       
    "baseUrl": "./src",                   
    "rootDirs": ["./"],   
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true  
  },
  "exclude": ["node_modules", "tempAsset","access*.*","error*.*"]
}